{
  "version": 2,
  "functions": {
    "api/*.py": { "runtime": "python3.11" }
  },
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/", "dest": "api/teal.py" },
    { "src": "/approval", "dest": "api/teal.py" },
    { "src": "/clear", "dest": "api/teal.py" }
  ]
}
